{% trans_default_domain 'messages' %}

{% extends 'AdminBundle:Expertise:layout_editor.html.twig' %}

{% block title %}{{ parent() }} - {{ "expertise.content.feature.update"|trans|capitalize }} {% endblock %}
{% block breadcrumbmodule %}
    {{ parent() }}
    <a href=" {{ url('admin_expertise_feature_list') }} ">
       {{ 'expertise.content.feature.list'|trans|capitalize }}
    </a>
    <i class="fa fa-chevron-right"></i>
    <em>{{ "expertise.content.feature.update"|trans|capitalize }}</em>
{% endblock %}

{% block content %}
    <div id="page_content_inner">
        {% include 'AdminBundle:Expertise:form_feature.html.twig' with { 'form' : form, 'feature' : feature } %}
    </div>
{% endblock %}
