{% trans_default_domain 'messages' %}

{% extends 'CharityBundle::layout_editor.html.twig' %}

{% block title %}{{ parent() }} - {{ "charity.content.category.update"|trans|capitalize }} {% endblock %}

{% block breadcrumbmodule %}
	{{ parent() }}
    <a href=" {{ url('admin_charity_category_list') }} ">
       {{ 'charity.content.category.list'|trans|capitalize }}
    </a>
	<i class="material-icons">keyboard_arrow_right</i>
	<em>{{ "charity.content.category.update"|trans|capitalize }}</em>
{% endblock %}

{% block content %}
    <div id="page_content_inner">
        {% include 'CharityBundle:Category:form.html.twig' with { 'form': form, 'category': category } %}
    </div>
{% endblock %}

{% block modal %}
    <div class="uk-modal" id="choose_files_modal">
        <div class="uk-modal-dialog uk-modal-dialog-large" id="choose_files_modal_dialog">
        </div>
    </div>
{% endblock %}

